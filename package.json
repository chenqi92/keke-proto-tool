{
  "name": "keke-proto-tool",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "description": "跨平台的网络报文工作站，集连接调试、协议解析、规则/插件扩展、数据筛选存储、AI 辅助与批量导出为一体",
  "author": {
    "name": "programApe",
    "email": "hi@kkape.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/chenqi92/keke-proto-tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/chenqi92/keke-proto-tool.git"
  },
  "bugs": {
    "url": "https://github.com/chenqi92/keke-proto-tool/issues"
  },
  "keywords": [
    "protocol",
    "network",
    "parser",
    "tauri",
    "rust",
    "react",
    "typescript",
    "cross-platform",
    "desktop-app"
  ],
  "scripts": {
    "dev": "vite",
    "dev:tauri": "tauri dev",
    "build": "tsc && vite build",
    "build:tauri": "node scripts/build.js",
    "build:clean": "node scripts/build.js --clean",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "version:current": "node scripts/version.js current",
    "version:patch": "node scripts/version.js bump patch",
    "version:minor": "node scripts/version.js bump minor",
    "version:major": "node scripts/version.js bump major",
    "version:set": "node scripts/version.js set",
    "version:patch:strict": "node scripts/version.js bump patch --strict",
    "version:minor:strict": "node scripts/version.js bump minor --strict",
    "version:major:strict": "node scripts/version.js bump major --strict",
    "version:set:strict": "node scripts/version.js set --strict",
    "release": "node scripts/release.js",
    "release:dry": "node scripts/release.js --dry-run"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.1.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.363.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^3.0.5",
    "react-router-dom": "^6.22.3",
    "tailwind-merge": "^2.2.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.1.0",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
