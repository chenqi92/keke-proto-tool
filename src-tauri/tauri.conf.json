{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "ProtoTool",
  "version": "../package.json",
  "identifier": "com.keke.proto-tool",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3001",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "ProtoTool",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "titleBarStyle": "Overlay",
        "decorations": true,
        "transparent": false,
        "shadow": true,
        "center": true,
        "focus": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "visible": true,
        "backgroundColor": "#2B2B2B"
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": ["en-US", "zh-CN"],
        "template": null
      },
      "nsis": {
        "displayLanguageSelector": true,
        "languages": ["English", "SimpChinese"],
        "template": null
      }
    }
  }
}
